UXTerm*font: xft:Inconsolata Nerd Font:medium:size=10.5 
UXTerm*internalBorder: 12 
xterm*iconPixmap: /home/nick/Pictures/icons/terminal.xbm
UXTerm.scaleHeigh: 20

URxvt*font:			xft:Inconsolata For Powerline:medium:size=10.5
URxvt*iconFile: /home/nick/Pictures/icons/battery.svg

URxvt*cursorUnderline: false 

URxvt*scrollBar: false
URxvt*fading: 20
URxvt*internalBorder: 12 
Urxvt*depth: 32
Urxvt*background: rgba:0000/0000/0200/c800


URxvt.keysym.C-Up:     font-size:increase
URxvt.keysym.C-Down:   font-size:decrease
URxvt.keysym.C-S-Up:   font-size:incglobal
URxvt.keysym.C-S-Down: font-size:decglobal
URxvt.keysym.C-equal:  font-size:reset
URxvt.keysym.C-slash:  font-size:show
